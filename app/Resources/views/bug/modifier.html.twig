{% extends '::layout/layoutAdmin.html.twig' %}

{% block body %}
    
   <h1>Reclamation list</h1>

    <table class="table table-striped table-hover">
        <thead>
            <tr>
          
                <th>Client</th>
                <th>Titre</th>
                <th>Description</th>
                <th>Date</th>
                <th>Application</th>
                <th>Module</th>
                <th>Image</th>
               
            </tr>
        </thead>
        <tbody>
          {% for bug in bugs %}
            <tr>
                
               
                <td>{{ bug.client }}</td>
                <td>{{ bug.titre }}</td>
                <td>{{ bug.description }}</td>     
                <td>{% if bug.date %}{{ bug.date|date('Y-m-d') }}{% endif %}</td>
                <td>{{ bug.nom1 }}</td>
                <td>{{ bug.titre1 }}</td>
               
                <td> <img src="{{asset('uploads/'~bug.path)}}" alt="DevAndClick" width="300" height="300"></td>
            
            </tr>
        {% endfor %}
        </tbody>
    </table>


{% endblock %}
























