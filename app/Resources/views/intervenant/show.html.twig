{% extends '::layout/layoutAdmin.html.twig' %}

{% block body %}
    <h1>Intervenant</h1>

   <table class="table table-striped table-hover">
        <tbody>
       
            <tr>
                <th>Nom</th>
                <td>{{ intervenant.nom4 }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('intervenant_modifier') }}"class="btn btn-info">Retour a la page Modifier</a>
        </li>
        <li>
            <a href="{{ path('intervenant_edit', { 'id': intervenant.id }) }}" class="btn btn-success">Modifier</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" class="btn btn-danger" value="Supprimer" />
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
